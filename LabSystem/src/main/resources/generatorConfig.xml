<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 设置mysql驱动路径 -->
    <!-- 此处指定生成针对MyBatis3的DAO -->
    <context id="context" targetRuntime="MyBatis3">
        <!-- 分页插件-->
        <plugin type="mybatis.generator.plugin.mysql.PaginationPlugin"/>
        <plugin type="mybatis.generator.plugin.InsertBatchPlugin"/>
        <plugin type="mybatis.generator.plugin.AnnotationPlugin"/>
        <!-- 去除自动生成的注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- jdbc连接信息 -->

        <jdbcConnection
                driverClass="com.mysql.jdbc.Driver"
                connectionURL="jdbc:mysql://localhost:3306/manage_system"
                userId="root"
                password="dinpper"/>
        <!-- 生成vo对象(*.java)及生成用于查询的Example对象(*Example.java) -->
        <javaModelGenerator
                targetPackage="com.example.labsystem.orm.po"
                targetProject="src/main/java"/>
        <!-- dao配置文件 (*Mapper.xml) -->
        <sqlMapGenerator
                targetPackage="mappers"
                targetProject="src/main/resources"/>
        <!-- 生成DAO的类文件 (*Mapper.java) -->
        <javaClientGenerator
                targetPackage="com.example.labsystem.orm.dao"
                targetProject="src/main/java"
                type="xmlmapper"/>
